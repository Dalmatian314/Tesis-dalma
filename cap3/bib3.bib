issn = {2041-1723},
journal = {Nature communications},
mendeley-groups = {Organic Solar Cells},
pages = {1446},
pmid = {23385590},
publisher = {Nature Publishing Group},
title = {{A polymer tandem solar cell with 10.6{\%} power conversion efficiency.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3660643{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {4},
year = {2013}
}


@article{Oviedo2012,
abstract = {On the basis of a time-dependent self-consistent density functional tight-binding (TD-DFTB) approach, we present a novel method able to capture the differences between direct and indirect photoinjection mechanisms in a fully atomistic picture. A model anatase TiO2 nanoparticle (NP) functionalized with different dyes has been chosen as the object of study. We show that a linear dependence of the rate of electron injection with respect to the square of the applied field intensity can be viewed as a signature of a direct electron injection mechanism. In addition, we show that the nature of the photoabsorption process can be understood in terms of orbital population dynamics occurring during photoabsorption. Dyes involved in both direct (type-I) and indirect (type-II) mechanisms were studied to test the predictive power of this method.$\backslash$nOn the basis of a time-dependent self-consistent density functional tight-binding (TD-DFTB) approach, we present a novel method able to capture the differences between direct and indirect photoinjection mechanisms in a fully atomistic picture. A model anatase TiO2 nanoparticle (NP) functionalized with different dyes has been chosen as the object of study. We show that a linear dependence of the rate of electron injection with respect to the square of the applied field intensity can be viewed as a signature of a direct electron injection mechanism. In addition, we show that the nature of the photoabsorption process can be understood in terms of orbital population dynamics occurring during photoabsorption. Dyes involved in both direct (type-I) and indirect (type-II) mechanisms were studied to test the predictive power of this method.},
author = {Oviedo, Mar\'{\i}a Bel\'{e}n and Zarate, Ximena and Negre, Christian F. A. and Schott, Eduardo and Arratia-P\'{e}rez, Ramiro and S\'{a}nchez, Cristi\'{a}n G.},
doi = {10.1021/jz300880d},
file = {:home/bethania/charly/papersbel/jz300880d.pdf:pdf},
journal = {The Journal of Physical Chemistry Letters},
mendeley-groups = {Biblio},
pages = {2548--2555},
title = {{Quantum Dynamical Simulations as a Tool for Predicting Photoinjection Mechanisms in Dye-Sensitized TiO 2 Solar Cells}},
volume = {3},
year = {2012}
}

